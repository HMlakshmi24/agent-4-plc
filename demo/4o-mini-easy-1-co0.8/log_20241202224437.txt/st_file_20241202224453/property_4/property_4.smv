-- Model
MODULE main
	VAR
		loc : {init_pv, end, loop_start, prepare_BoC, l_main_call, prepare_EoC, verificationLoop_VerificationLoop_l3};
		instance_R2_Estop_IN : boolean; -- frozen
		instance_R2_Estop_OUT : boolean; -- frozen
		EoC : boolean; -- frozen
		BoC : boolean; -- frozen
	
	ASSIGN
		-- CFA structure (loc)
		init(loc) := init_pv;
		next(loc) := case
			loc = init_pv & (TRUE) : loop_start;
			loc = loop_start & (TRUE) : prepare_BoC;
			loc = prepare_BoC & (TRUE) : l_main_call;
			loc = verificationLoop_VerificationLoop_l3 & (TRUE) : prepare_EoC;
			loc = prepare_EoC & (TRUE) : loop_start;
			loc = l_main_call & (TRUE) : verificationLoop_VerificationLoop_l3;
			TRUE: loc;
		esac;
		
		init(instance_R2_Estop_IN) := FALSE;
		next(instance_R2_Estop_IN) := case
			loc = loop_start & (TRUE) : {TRUE, FALSE};
			TRUE  : instance_R2_Estop_IN;
		esac;
		init(instance_R2_Estop_OUT) := FALSE;
		next(instance_R2_Estop_OUT) := case
			loc = l_main_call & (TRUE) : !(instance_R2_Estop_IN);
			TRUE  : instance_R2_Estop_OUT;
		esac;
		init(EoC) := FALSE;
		next(EoC) := case
			loc = verificationLoop_VerificationLoop_l3 & (TRUE) : TRUE;
			loc = prepare_EoC & (TRUE) : FALSE;
			TRUE  : EoC;
		esac;
		init(BoC) := FALSE;
		next(BoC) := case
			loc = loop_start & (TRUE) : TRUE;
			loc = prepare_BoC & (TRUE) : FALSE;
			TRUE  : BoC;
		esac;

-- Requirement
CTLSPEC AG((EoC) -> ((instance_R2_Estop_OUT) = (!(instance_R2_Estop_IN))));
