-- Model
MODULE main
	VAR
		loc : {init_pv, end, loop_start, prepare_BoC, l_main_call, prepare_EoC, verificationLoop_VerificationLoop_l4};
		instance_E_Left : boolean; -- frozen
		instance_PI_LED_Red : boolean; -- frozen
		EoC : boolean; -- frozen
		BoC : boolean; -- frozen
	
	ASSIGN
		-- CFA structure (loc)
		init(loc) := init_pv;
		next(loc) := case
			loc = init_pv & (TRUE) : loop_start;
			loc = loop_start & (TRUE) : prepare_BoC;
			loc = prepare_BoC & (TRUE) : l_main_call;
			loc = verificationLoop_VerificationLoop_l4 & (TRUE) : prepare_EoC;
			loc = prepare_EoC & (TRUE) : loop_start;
			loc = l_main_call & (TRUE) : verificationLoop_VerificationLoop_l4;
			TRUE: loc;
		esac;
		
		init(instance_E_Left) := FALSE;
		next(instance_E_Left) := case
			loc = loop_start & (TRUE) : {TRUE, FALSE};
			TRUE  : instance_E_Left;
		esac;
		init(instance_PI_LED_Red) := FALSE;
		next(instance_PI_LED_Red) := case
			loc = l_main_call & (TRUE) : instance_E_Left;
			TRUE  : instance_PI_LED_Red;
		esac;
		init(EoC) := FALSE;
		next(EoC) := case
			loc = verificationLoop_VerificationLoop_l4 & (TRUE) : TRUE;
			loc = prepare_EoC & (TRUE) : FALSE;
			TRUE  : EoC;
		esac;
		init(BoC) := FALSE;
		next(BoC) := case
			loc = loop_start & (TRUE) : TRUE;
			loc = prepare_BoC & (TRUE) : FALSE;
			TRUE  : BoC;
		esac;

-- Requirement
CTLSPEC AG((EoC) -> ((instance_PI_LED_Red) = (instance_E_Left)));
