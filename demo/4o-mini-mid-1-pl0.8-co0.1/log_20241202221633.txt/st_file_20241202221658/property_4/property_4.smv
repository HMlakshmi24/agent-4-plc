-- Model
MODULE main
	VAR
		loc : {init_pv, end, loop_start, prepare_BoC, l_main_call, prepare_EoC, verificationLoop_VerificationLoop_l4};
		instance_PI_Button_Right : boolean; -- frozen
		instance_A_Motor_Right : boolean; -- frozen
		EoC : boolean; -- frozen
		BoC : boolean; -- frozen
	
	ASSIGN
		-- CFA structure (loc)
		init(loc) := init_pv;
		next(loc) := case
			loc = init_pv & (TRUE) : loop_start;
			loc = loop_start & (TRUE) : prepare_BoC;
			loc = prepare_BoC & (TRUE) : l_main_call;
			loc = verificationLoop_VerificationLoop_l4 & (TRUE) : prepare_EoC;
			loc = prepare_EoC & (TRUE) : loop_start;
			loc = l_main_call & (TRUE) : verificationLoop_VerificationLoop_l4;
			TRUE: loc;
		esac;
		
		init(instance_PI_Button_Right) := FALSE;
		next(instance_PI_Button_Right) := case
			loc = loop_start & (TRUE) : {TRUE, FALSE};
			TRUE  : instance_PI_Button_Right;
		esac;
		init(instance_A_Motor_Right) := FALSE;
		next(instance_A_Motor_Right) := case
			loc = l_main_call & (TRUE) : !(instance_PI_Button_Right);
			TRUE  : instance_A_Motor_Right;
		esac;
		init(EoC) := FALSE;
		next(EoC) := case
			loc = verificationLoop_VerificationLoop_l4 & (TRUE) : TRUE;
			loc = prepare_EoC & (TRUE) : FALSE;
			TRUE  : EoC;
		esac;
		init(BoC) := FALSE;
		next(BoC) := case
			loc = loop_start & (TRUE) : TRUE;
			loc = prepare_BoC & (TRUE) : FALSE;
			TRUE  : BoC;
		esac;

-- Requirement
CTLSPEC AG((EoC) -> ((instance_A_Motor_Right) = (!(instance_PI_Button_Right))));
