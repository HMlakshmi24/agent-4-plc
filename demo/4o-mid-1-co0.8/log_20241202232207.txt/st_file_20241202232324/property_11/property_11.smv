-- Model
MODULE main
	VAR
		loc : {init_pv, end, loop_start, prepare_BoC, l_main_call, prepare_EoC, verificationLoop_VerificationLoop_l11};
		instance_CYCLE : signed word[16]; -- frozen
		instance_UPDATED_CYCLE : signed word[16]; -- frozen
		EoC : boolean; -- frozen
		BoC : boolean; -- frozen
		-- Random vars for nondeterministic INTs
		__random_instance_CYCLE : signed word[16];
	
	ASSIGN
		-- CFA structure (loc)
		init(loc) := init_pv;
		next(loc) := case
			loc = init_pv & (TRUE) : loop_start;
			loc = loop_start & (TRUE) : prepare_BoC;
			loc = prepare_BoC & (TRUE) : l_main_call;
			loc = verificationLoop_VerificationLoop_l11 & (TRUE) : prepare_EoC;
			loc = prepare_EoC & (TRUE) : loop_start;
			loc = l_main_call & (TRUE) : verificationLoop_VerificationLoop_l11;
			TRUE: loc;
		esac;
		
		init(instance_CYCLE) := 0sd16_0;
		next(instance_CYCLE) := case
			loc = loop_start & (TRUE) : __random_instance_CYCLE; -- Nondeterministic
			TRUE  : instance_CYCLE;
		esac;
		init(instance_UPDATED_CYCLE) := 0sd16_0;
		next(instance_UPDATED_CYCLE) := case
			loc = l_main_call & (TRUE) : (instance_CYCLE) + (0sd16_1);
			TRUE  : instance_UPDATED_CYCLE;
		esac;
		init(EoC) := FALSE;
		next(EoC) := case
			loc = verificationLoop_VerificationLoop_l11 & (TRUE) : TRUE;
			loc = prepare_EoC & (TRUE) : FALSE;
			TRUE  : EoC;
		esac;
		init(BoC) := FALSE;
		next(BoC) := case
			loc = loop_start & (TRUE) : TRUE;
			loc = prepare_BoC & (TRUE) : FALSE;
			TRUE  : BoC;
		esac;

-- Requirement
CTLSPEC AG((EoC) -> ((instance_UPDATED_CYCLE) = ((instance_CYCLE) + (0sd16_1))));
